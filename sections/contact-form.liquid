<div class="flex flex-col item-center my-8">\
  {% form 'contact' %}
    <label for="contact_name">Name</label>
    <input type="text" name="contact[name]" id="contact_name">

    
  {% endform %}
</div>


{% schema %}
  {
    "name": "contact",
    "presets": [
      {
        "name": "Contact Form"
      }
    ]
  }
{% endschema %}